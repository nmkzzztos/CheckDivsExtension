{
    "manifest_version": 2,
    "name": "Abhacker",
    "version": "1.0",
    "permissions": ["webNavigation", "activeTab", "<all_urls>", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
}